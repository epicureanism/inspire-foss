<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>inspire-foss-workspace</artifactId>
  <packaging>deegree-workspace</packaging>
  <name>inspire-foss-workspace</name>

  <parent>
    <groupId>org.deegree</groupId>
    <artifactId>deegree-workspace-parent</artifactId>
    <version>3.1-pre12</version>
  </parent>

  <repositories>
    <repository>
      <id>deegree-repo</id>
      <url>http://artefacts.deegree.org/repo</url>
      <releases>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>occamlabs-repo</id>
      <url>https://jenkins.occamlabs.de/artifactory/repo</url>
      <releases>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </repository>
  </repositories>

  <distributionManagement>
    <repository>
      <id>occamlabs-repo</id>
      <url>https://jenkins.occamlabs.de/artifactory/libs-release-local</url>
    </repository>
    <snapshotRepository>
      <id>occamlabs-repo</id>
      <url>https://jenkins.occamlabs.de/artifactory/libs-snapshot-local</url>
    </snapshotRepository>
  </distributionManagement>

  <dependencies>
    <dependency>
      <groupId>org.deegree</groupId>
      <artifactId>deegree-workspace-base</artifactId>
      <version>3.1-pre12</version>
      <type>deegree-workspace</type>
    </dependency>
  </dependencies>

</project>
