					<xsd:element maxOccurs="unbounded" minOccurs="0" name="postalDescriptor" type="gml:ReferenceType">
						<xsd:annotation>
							<xsd:appinfo>
								<deegreewfs:Content type="AD:PostalDescriptor">
									<deegreewfs:Relation>
										<deegreewfs:From>
											<deegreewfs:MappingField field="ad_postal_id" type="VARCHAR"/>
										</deegreewfs:From>
										<deegreewfs:To>
											<deegreewfs:MappingField field="id" type="VARCHAR" table="ad_postal"/>
										</deegreewfs:To>
									</deegreewfs:Relation>
								</deegreewfs:Content>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="thoroughfareName" type="gml:ReferenceType">
						<xsd:annotation>
							<xsd:appinfo>
								<deegreewfs:Content type="AD:ThoroughfareName">
									<deegreewfs:Relation>
										<deegreewfs:From>
											<deegreewfs:MappingField field="ad_thoroughfare_id" type="VARCHAR"/>
										</deegreewfs:From>
										<deegreewfs:To>
											<deegreewfs:MappingField field="id" type="VARCHAR" table="ad_thoroughfare"/>
										</deegreewfs:To>
									</deegreewfs:Relation>
								</deegreewfs:Content>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
